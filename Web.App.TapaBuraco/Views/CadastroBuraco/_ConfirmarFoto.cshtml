@{
    Layout = null;
    var uriFoto = (string)ViewBag.UriImg;
    TempData["uriFoto"] = uriFoto;
}


@using (Html.BeginForm("PreencherDados", "CadastroBuraco", FormMethod.Get))
{
<div class="card" style="width: 18rem;">
    <img class="card-img-top" src="@uriFoto" alt="Foto Buraco">
    <div class="card-body">
        <div class="form-row">
            <div class="form-group clearfix">
            </div>
            <div class="form-group clearfix">
                <button type="button" onclick="window.location.reload()" class="btn btn-danger">Cancelar</button>
                <button type="submit" class="btn btn-success">Confirmar</button>
            </div>
        </div>
    </div>
</div>
}